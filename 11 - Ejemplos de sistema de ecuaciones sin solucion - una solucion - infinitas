    "plt.figure() #creamos nuestra figura donde mostraremos nuestro grafico\n",
    "plt.plot(x, y_1) #queremos graficar x jutno a y_1\n",
    "plt.plot(x, y_2) #queremos graficar x jutno a y_2\n",
    "plt.plot(x, y_3) #queremos graficar x jutno a y_3\n",
    "\n",
    "plt.xlim(-8,8) #agregamos l;ímites a los ejes del grafico\n",
    "plt.ylim(-8,8) #agregamos l;ímites a los ejes del grafico\n",
    "\n",
    "plt.axvline(x=0, color=\"grey\") #agregamos lineas de division x\n",
    "plt.axhline(y=0, color=\"grey\") #agregamos lineas de division y\n",
    "plt.show() #mostramos nuestro grafico\n",
    "\n"
